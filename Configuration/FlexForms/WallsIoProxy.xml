<T3DataStructure>
    <sheets>
        <sDEF>
            <ROOT>
                <sheetTitle>LLL:EXT:walls_io_proxy/Resources/Private/Language/FlexForms.xlf:sheetGeneral</sheetTitle>
                <type>array</type>
                <el>
                    <accessToken>
                        <label>LLL:EXT:walls_io_proxy/Resources/Private/Language/FlexForms.xlf:accessToken</label>
                        <config>
                            <type>input</type>
                            <size>30</size>
                            <eval>trim,required</eval>
                        </config>
                    </accessToken>
                    <requestType>
                        <label>LLL:EXT:walls_io_proxy/Resources/Private/Language/FlexForms.xlf:requestType</label>
                        <config>
                            <type>select</type>
                            <renderType>selectSingle</renderType>
                            <items>
                                <numIndex index="0">
                                    <numIndex index="0">
                                        LLL:EXT:walls_io_proxy/Resources/Private/Language/FlexForms.xlf:requestType.posts
                                    </numIndex>
                                    <numIndex index="1">JWeiland\WallsIoProxy\Request\PostsRequest</numIndex>
                                </numIndex>
                                <numIndex index="1">
                                    <numIndex index="0">
                                        LLL:EXT:walls_io_proxy/Resources/Private/Language/FlexForms.xlf:requestType.changedPosts
                                    </numIndex>
                                    <numIndex index="1">JWeiland\WallsIoProxy\Request\Posts\ChangedRequest</numIndex>
                                </numIndex>
                            </items>
                        </config>
                    </requestType>
                    <entriesToLoad>
                        <label>LLL:EXT:walls_io_proxy/Resources/Private/Language/FlexForms.xlf:entriesToLoad</label>
                        <description>
                            LLL:EXT:walls_io_proxy/Resources/Private/Language/FlexForms.xlf:entriesToLoad.description
                        </description>
                        <config>
                            <type>input</type>
                            <size>10</size>
                            <default>24</default>
                            <max>3</max>
                            <range>
                                <lower>0</lower>
                                <upper>999</upper>
                            </range>
                            <slider>
                                <step>8</step>
                                <width>300</width>
                            </slider>
                            <eval>int,required</eval>
                        </config>
                    </entriesToLoad>
                    <entriesToShow>
                        <label>LLL:EXT:walls_io_proxy/Resources/Private/Language/FlexForms.xlf:entriesToShow</label>
                        <config>
                            <type>input</type>
                            <size>10</size>
                            <default>8</default>
                            <max>3</max>
                            <range>
                                <lower>0</lower>
                                <upper>999</upper>
                            </range>
                            <slider>
                                <step>8</step>
                                <width>300</width>
                            </slider>
                            <eval>int,required</eval>
                        </config>
                    </entriesToShow>
                    <showWallsSince>
                        <label>LLL:EXT:walls_io_proxy/Resources/Private/Language/FlexForms.xlf:showWallsSince</label>
                        <description>
                            LLL:EXT:walls_io_proxy/Resources/Private/Language/FlexForms.xlf:showWallsSince.description
                        </description>
                        <config>
                            <type>input</type>
                            <size>10</size>
                            <default>365</default>
                            <max>5</max>
                            <eval>int,required</eval>
                        </config>
                    </showWallsSince>
                </el>
            </ROOT>
        </sDEF>
    </sheets>
</T3DataStructure>
