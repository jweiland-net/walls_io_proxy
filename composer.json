{
	"name": "jweiland/walls-io-proxy",
	"type": "typo3-cms-extension",
	"description": "Cache and Proxy for walls.io, so no Cookie will be set on Client",
	"license": "GPL-2.0-or-later",
	"keywords": [
		"typo3",
		"TYPO3 CMS",
		"walls",
		"walls.io",
		"cache",
		"proxy"
	],
	"homepage": "https://jweiland.net",
	"authors": [
		{
			"name": "Stefan Froemken",
			"email": "sfroemken@jweiland.net",
			"role": "Lead Developer"
		},
		{
			"name": "Hoja Mustaffa Abdul Latheef",
			"email": "hlatheef@jweiland.net",
			"role": "Senior Developer"
		}
	],
	"support": {
		"email": "projects@jweiland.net",
		"issues": "https://github.com/jweiland-net/walls_io_proxy/issues",
		"source": "https://github.com/jweiland-net/walls_io_proxy"
	},
	"require": {
		"typo3/cms-core": "^12.4.17"
	},
	"require-dev": {
		"friendsofphp/php-cs-fixer": "^3.14",
		"phpunit/phpunit": "^9.6",
		"typo3/coding-standards": "^0.6",
		"typo3/testing-framework": "^7.0.2"
	},
	"replace": {
		"typo3-ter/walls_io_proxy": "self.version"
	},
	"autoload": {
		"psr-4": {
			"JWeiland\\WallsIoProxy\\": "Classes"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"JWeiland\\WallsIoProxy\\Tests\\": "Tests"
		}
	},
	"config": {
		"sort-packages": true,
		"vendor-dir": ".Build/vendor",
		"bin-dir": ".Build/bin",
		"allow-plugins": {
			"typo3/class-alias-loader": true,
			"typo3/cms-composer-installers": true
		}
	},
	"extra": {
		"typo3/cms": {
			"extension-key": "walls_io_proxy",
			"app-dir": ".Build",
			"web-dir": ".Build/public"
		}
	},
	"scripts": {
		"ci:php:fix": ".Build/bin/php-cs-fixer --config=Build/php-cs-fixer/config.php fix Classes Tests",
		"ci:php:lint": "find *.php Classes Configuration Tests -name '*.php' -print0 | xargs -0 -n 1 -P 4 php -l",
		"ci:php:fixer": ".Build/bin/php-cs-fixer --config=Build/php-cs-fixer/config.php fix --dry-run -v --show-progress=dots --diff Classes Tests"
	}
}
